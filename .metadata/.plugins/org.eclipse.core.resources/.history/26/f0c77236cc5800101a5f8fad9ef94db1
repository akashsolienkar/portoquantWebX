package com.example.portoquant.assets;

import java.util.ArrayList;

/**
 * Represents a commodity asset within the portfolio.
 * <p>
 * Extends the abstract {@link Asset} class, specialized for commodities.
 * </p>
 * 
 * @author akashsolienkar
 */
public class Commodity extends Asset {

    private ArrayList<String> commodities;

    /**
     * Constructs a Commodity asset with the specified price.
     * Initializes the asset type as COMMODITY.
     * 
     * @param price the total price of the commodity asset
     */
    public Commodity(double price) {
        super(Asset.AssetType.COMMODITY.toString(), Asset.AssetType.COMMODITY);
        this.setTotalPrice(price);
    }

    /**
     * Calculates the expected return for this commodity asset.
     * Implementation to be provided.
     */
    @Override
    public void calculateExpectedReturn() {
        // TODO Auto-generated method stub
    }

    /**
     * Calculates the volatility for this commodity asset.
     * Implementation to be provided.
     */
    @Override
    public void calculateVolatility() {
        // TODO Auto-generated method stub
    }
}

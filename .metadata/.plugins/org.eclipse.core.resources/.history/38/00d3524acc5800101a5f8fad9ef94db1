package com.example.portoquant.assets;

import java.util.ArrayList;

/**
 * Represents a mutual fund asset within the portfolio.
 * Extends the abstract Asset class, specialized for mutual funds.
 * 
 * @author akashsolienkar
 */
public class MutualFunds extends Asset {

    private ArrayList<String> MutualFunds;

    /**
     * Constructs a MutualFunds asset with the specified price.
     * Initializes the asset type as MUTUAL_FUND.
     * 
     * @param price the total price of the mutual fund asset
     */
    public MutualFunds(double price) {
        super(Asset.AssetType.MUTUAL_FUND.toString(), Asset.AssetType.MUTUAL_FUND);
        this.setTotalPrice(price);
    }

    /**
     * Calculates the expected return for this mutual fund asset.
     * Implementation pending.
     */
    @Override
    public void calculateExpectedReturn() {
        // TODO Auto-generated method stub
    }

    /**
     * Calculates the volatility for this mutual fund asset.
     * Implementation pending.
     */
    @Override
    public void calculateVolatility() {
        // TODO Auto-generated method stub
    }
}

package com.example.portoquant.assets;

import java.util.ArrayList;

/**
 * Represents a real estate asset within the portfolio.
 * Extends the abstract {@link Asset} class, specialized for Real estate assets.
 * 
 * @author akashsolienkar
 */
public class RealEstate extends Asset {

    private ArrayList<String> RealEstate;

    /**
     * Constructs a RealEstate asset with the specified price.
     * Initializes the asset type as REAL_ESTATE.

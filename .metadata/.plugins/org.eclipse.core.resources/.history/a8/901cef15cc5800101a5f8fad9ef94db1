package com.example.portoquant.assets;

import java.util.ArrayList;

/**
 * Represents a cash asset within the portfolio.
 * <p>
 * Extends the abstract {@link Asset} class, specialized for cash assets.
 * </p>
 * 
 * @author akashsolienkar
 */
public class Cash extends Asset {

    private ArrayList<String> bonds;

    /**
     * Constructs a Cash asset with the specified price.
     * Initializes the asset type as CASH.
     *
     * @param price the total price of the cash asset
     */
    public Cash(double price) {
        super(Asset.AssetType.CASH.toString(), Asset.AssetType.CASH);
        this.setTotalPrice(price);
    }

    /**
     * Calculates the expected return for this cash asset.
     * Implementation to be provided.
     */
    @Override
    public void calculateExpectedReturn() {
        // TODO Auto-generated method stub
    }

    /**
     * Calculates the volatility for this cash asset.
     * Implementation to be provided.
     */
    @Override
    public void calculateVolatility() {
        // TODO Auto-generated method stub
    }
}
